{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "linebreak-style": "off",
    "class-methods-use-this": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off",
    "arrow-parens": "off",
    "no-plusplus": "off",
    "no-multi-assign": "off",
    "import/prefer-default-export": "off"
  }
}